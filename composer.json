{
    "name": "elixir/elixir-stdlib",
    "description": "Component stdlib",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "stdlib"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-stdlib",
    "repositories": [
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\STDLib\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\STDLib\\": "tests/"
        }
    },
    "suggest": {
        "elixir/elixir-di": "*",
        "elixir/elixir-http": "*"
    }
}
