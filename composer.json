{
    "name": "elixir/elixir-stdlib",
    "description": "Component stdlib",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "license": "BSD-3-Clause",
    "keywords": [
        "elixir",
        "stdlib"
    ],
    "homepage": "https://github.com/cedtanghe/elixir-stdlib",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cedtanghe/elixir-di"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Elixir\\STDLib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Test\\STDLib\\": "tests/"
        }
    },
    "suggest": {
        "elixir/elixir-di": "*"
    }
}